    <!-- Vertical connecting walls -->
    <model name="v1"><static>true</static><pose>-8 3 1 0 0 0</pose>
      <link name="link">
        <collision name="c"><geometry><box><size>0.4 4 2</size></box></geometry></collision>
        <visual name="v"><geometry><box><size>0.4 4 2</size></box></geometry>
