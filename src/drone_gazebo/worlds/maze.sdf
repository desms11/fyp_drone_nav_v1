        <visual name="v"><geometry><box><size>10 0.4 2</size></box></geometry>
          <material><ambient>0.8 0.8 0.8 1</ambient><diffuse>0.8 0.8 0.8 1</diffuse></material></visual>
      </link></model>

    <!-- Row 5 (mid) -->
    <model name="w9"><static>true</static><pose>-4 -0.75 1 0 0 0</pose>
      <link name="link">
        <collision name="c"><geometry><box><size>10 0.4 2</size></box></geometry></collision>
